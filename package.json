{

          "name": "scratch-for-discord-bot",

          "version": "1.0.0",

          "main": "boot.js",

          "scripts": {

                    "start": "npm i && node .",

                    "node-update": "npm i --save-dev node@17 && npm config set prefix=$(pwd)/node_modules/node && export PATH=$(pwd)/node_modules/node/bin:$PATH",

                    "node-clean": "rm -rf node_modules && rm package-lock.json && npm cache clear --force && npm cache clean --force && npm i"

          },

          "dependencies": {

                    "@haileybot/captcha-generator": "^1.7.0",

                    "discord-backup": "^3.2.1",

                    "discord-logs": "^2.0.1",

                    "discord.js": "^13.6.0",

                    "djs-games": "^2.1.10",

                    "easy-json-database": "^1.5.0",

                    "lyrics-finder": "^21.7.0",

                    "millisecond-converter": "^1.4.3",

                    "moment": "latest",

                    "os-utils": "0.0.14",

                    "process": "^0.11.10",

                    "qrcode": "^1.5.0",

                    "smartestchatbot": "^2.0.2"

          },

          "devDependencies": {

                    "node": "^17"

          }

}

